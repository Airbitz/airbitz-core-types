{
  "name": "airbitz-core-types",
  "version": "0.0.1",
  "description": "Flow types for airbitz-core-js",
  "main": "indexAbcTypes.js",
  "scripts": {
    "precommit": "npm build",
    "build": "npm run flow && npm run lint",
    "flow": "flow",
    "updot": "updot",
    "prepare": "npm run build",
    "cover": "nyc --reporter=lcov --reporter=text --reporter=html --extension .js npm test",
    "lint --fix": "standard '*.js' 'src/**/*.js' 'test/**/*.js' --fix",
    "lint": "standard '*.js' 'src/**/*.js' 'test/**/*.js'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Airbitz/airbitz-core-types.git"
  },
  "keywords": [
    "airbitz",
    "flow",
    "types"
  ],
  "author": "Paul Puey <paul@airbitz.co>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Airbitz/airbitz-core-types/issues"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-preset-flow": "^6.23.0",
    "eslint-plugin-flowtype": "^2.34.1",
    "husky": "^0.14.3",
    "standard": "^10.0.2",
    "flow-bin": "^0.53.1"
  },
  "standard": {
    "parser": "babel-eslint",
    "plugins": [
      "flowtype"
    ]
  },
  "homepage": "https://github.com/Airbitz/airbitz-core-types#readme"
}
