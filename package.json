{
  "name": "airbitz-core-types",
  "version": "0.0.1",
  "description": "Flow types for airbitz-core-js",
  "keywords": [
    "airbitz",
    "flow",
    "types"
  ],
  "homepage": "https://github.com/Airbitz/airbitz-core-types#readme",
  "bugs": {
    "url": "https://github.com/Airbitz/airbitz-core-types/issues"
  },
  "license": "MIT",
  "author": "Paul Puey <paul@airbitz.co>",
  "main": "lib/indexAbcTypes.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Airbitz/airbitz-core-types.git"
  },
  "scripts": {
    "build": "flow-copy-source src lib && flow-remove-types -d lib/ src/",
    "precommit": "npm run flow && npm run lint",
    "flow": "flow",
    "lint": "standard '*.js'",
    "prepare": "npm run build && npm run flow && npm run lint"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-preset-flow": "^6.23.0",
    "eslint-plugin-flowtype": "^2.34.1",
    "flow-bin": "^0.53.1",
    "flow-copy-source": "^1.2.1",
    "flow-remove-types": "^1.2.1",
    "husky": "^0.14.3",
    "standard": "^10.0.2"
  },
  "standard": {
    "parser": "babel-eslint",
    "plugins": [
      "flowtype"
    ]
  }
}
